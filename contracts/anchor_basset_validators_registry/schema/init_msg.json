{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "InitMsg",
  "type": "object",
  "required": [
    "hub_contract",
    "registry"
  ],
  "properties": {
    "hub_contract": {
      "$ref": "#/definitions/String"
    },
    "registry": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/Validator"
      }
    }
  },
  "definitions": {
    "String": {
      "type": "string"
    },
    "Uint128": {
      "type": "string"
    },
    "Validator": {
      "type": "object",
      "required": [
        "address"
      ],
      "properties": {
        "address": {
          "$ref": "#/definitions/String"
        },
        "total_delegated": {
          "default": "0",
          "allOf": [
            {
              "$ref": "#/definitions/Uint128"
            }
          ]
        }
      }
    }
  }
}
